{
  "name": "@nut-tree-macpad/default-clipboard-provider",
  "version": "4.2.0",
  "description": "The @nut-tree-macpad/nut-js default clipboard provider based on clipboardy",
  "main": "dist/index",
  "typings": "dist/index",
  "scripts": {
    "coverage": "jest --coverage --runInBand --logHeapUsage",
    "coverage:clean": "rimraf coverage",
