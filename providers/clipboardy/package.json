    "compile": "pnpm run clean && tsc -p .",
    "publish:next": "pnpm publish --tag next --no-git-checks",
    "publish:release": "pnpm publish --no-git-checks",
    "prepublishOnly": "pnpm run compile"
  },
  "keywords": [
    "nut-js",
    "provider",
    "clipboard",
    "clipboardy"
  ],
  "author": {
    "name": "dry Software UG (haftungsbeschr√§nkt)",
    "email": "info@dry.software",
    "url": "https://dry.software"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "clipboardy": "2.3.0"
  },
  "devDependencies": {
    "@nut-tree-macpad/provider-interfaces": "workspace:*",
    "@nut-tree-macpad/configs": "workspace:*"
  },
  "peerDependencies": {
    "@nut-tree-macpad/nut-js": "workspace:*"
  }
